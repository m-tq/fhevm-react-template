{
  "name": "fhevm-vue-template",
  "version": "0.1.0",
  "description": "A Vue.js template for building privacy-preserving dApps with FHEVM",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "generate:abi": "node scripts/generateAbi.js",
    "deploy:local": "node scripts/deploy.js --network hardhat",
    "deploy:sepolia": "node scripts/deploy.js --network sepolia"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "ethers": "^6.8.0",
    "@fhevm-sdk": "npm:fhevm-sdk-zama@^0.1.0",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "lucide-vue-next": "^0.294.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.25",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@types/node": "^20.9.0"
  },
  "keywords": [
    "fhevm",
    "vue",
    "blockchain",
    "privacy",
    "encryption",
    "web3"
  ],
  "author": "Zama",
  "license": "MIT"
}